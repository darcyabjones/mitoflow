process {
  cpus = 1
  memory = 1.GB
  withLabel: download {
    errorStrategy = "retry"
    maxRetries = 10
  }
  withLabel: small_task {
    cpus = 1
    memory = 1.GB
  }
  withLabel: medium_task {
    cpus = 1
    memory = 2.GB
  }
  withLabel: biggish_task {
    cpus = 2
    memory = 4.GB
  }
  withLabel: big_task {
    cpus = 4
    memory = 8.GB
  }
}
